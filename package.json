    "@emotion/styled": "^11.3.0",
    "@fontsource/inter": "^4.5.0",
    "@metamask/jazzicon": "^2.0.0",
    "@openzeppelin/contracts": "^4.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@usedapp/core": "^0.4.7",
    "arb-ethers-web3-bridge": "^0.7.3",
    "ethers": "^5.4.0",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "truffle-hdwallet-provider": "^1.0.17",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "@ethersproject/abi": "5.2.0",
    "@ethersproject/contracts": "5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "jstest": "react-scripts test",
    "eject": "react-scripts eject",
    "test": "npm run test:arbitrum --network=$network",
    "installLocalArbitrum": "git clone -b sequencer2 https://github.com/offchainlabs/arbitrum.git",
    "startLocalEthereum": "cd arbitrum && git submodule update --init --recursive && yarn && yarn build && yarn docker:build:geth && yarn docker:geth",
    "startLocalArbitrum": "cd arbitrum && yarn demo:initialize && yarn demo:deploy",
    "compile:ethereum": "npx truffle compile",
    "compile:arbitrum": "npx truffle compile --config truffle-config.arbitrum.js",
    "migrate:ethereum": "npx truffle migrate",
    "migrate:arbitrum": "npx truffle migrate --config truffle-config.arbitrum.js --network $npm_config_network",
    "test:ethereum": "npx truffle test",
    "test:arbitrum": "npx truffle test --config truffle-config.arbitrum.js --network $npm_config_network"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "keywords": [
    "Arbitrum",
    "Ethereum",
    "Truffle",
    "Layer",
    "2"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.0.37",
    "@types/react-router-dom": "^5.1.8",
    "dotenv": "^10.0.0"
  }
}
